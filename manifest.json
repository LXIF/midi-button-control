{
  "manifest_version": 3,
  "name": "MIDI Button Control",
  "version": "1.0",
  "description": "Control any button with a MIDI input",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}